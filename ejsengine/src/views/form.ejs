<%-include('./partials/head') %> <%-include('./partials/header') %>
    <div class="container w-50 p-4">
        <h2 class="mt-3 mb-3"><%=subtitle %> </h2>
      <form action="/productos" method="POST" class="card card-body bg-secondary">
        <div class="form-group ">
          <input type="text" name="title" placeholder="Ingrese titulo" class="form-control mt-1" />
        </div>
        <div class="form-group">
          <input type="number" name="price" placeholder="Ingrese Precio" class="form-control mt-1" />
        </div>
        <div class="form-group">
          <input type="text" name="thumbnail" placeholder="Ingrese Url Imagen" class="form-control mt-1" />
        </div>
          <input type="submit" class="btn btn-warning mt-1"></input>
      </form>
      <a href="/productos"><button class="btn btn-success container mt-3">Ver Lista de Productos</button></a>
    </div>